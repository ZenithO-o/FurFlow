{
    "TimeZone": "US/Central",
    "DateString": "%Y-%m-%d %H:%M:%S",
    "Devices": {
        "dn1": {
            "location": "dealers_den"
        },
        "dn2": {
            "location": "hilton_lobby"
        }
    },
    "Locations": {
        "dealers_den": {
            "alias": "Dealer's Den",
            "capacity": 100
        },
        "hilton_lobby": {
            "alias": "Hilton Lobby",
            "capacity": 200
        }
    },
    "Serial": {
        "port": "COM4",
        "baudrate": "38400",
        "bytesize": 8,
        "stopbits": 1,
        "parity": "N",
        "xonxoff": true,
        "timeout": 4

    },
    "SensorReadouts": ["temperature", "aqi", "aqi_accuracy", "humidity", "pressure", "person_count"],
    "Output": ["capacity", "aqi", "temperature_f", "humidity", "pressure"],
    "Readouts": {
        "temperature": {
            "alias": "Temperature",
            "ending": "°C",
            "precision": 0
        },
        "temperature_f": {
            "alias": "Temperature",
            "ending": "°F",
            "precision": 0
        },
        "aqi": {
            "alias": "Air Quality",
            "ending": " AQI",
            "precision": 1
        },
        "aqi_accuracy": {
            "alias": "Air Quality Accuracy",
            "ending": null,
            "precision": 0
        },
        "humidity": {
            "alias": "Humidity",
            "ending": "%",
            "precision": 0
        },
        "pressure": {
            "alias": "Pressure",
            "ending": null,
            "precision": 0
        },
        "person_count": {
            "alias": "Person Count",
            "ending": null,
            "precision": 0
        },
        "capacity": {
            "alias": "Capacity",
            "ending": "%",
            "precision": 0
        }
    }
}